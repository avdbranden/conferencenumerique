<!-- Title -->

<div class="title text-center">
	<div class="container">
		<div class="row">
			<div class="col-xs-12">
				<div class="title-content">
					<h1>Sponsors</h1>
				</div>
			</div>
		</div>
	</div>
</div>

<!-- Proposition -->

<div id="proposition" class="text-center">
	<div class="container">
		<div class="row">
			<div class="col-md-offset-3 col-md-6 col-xs-12">
				<div class="proposition-content">
					<h4>Nous proposons aux sponsors pour un budget à négocier les contreparties suivantes:</h4>
					<ul class="text-left">
						<li>une présence sur le site</li>
						<li>une présence à l'évènement avec , selon les cas,  participation aux ateliers</li>
						<li>une présence dans le hall d'accueil avec une table dédiée</li>
						<li>une présence dans la salle de conférence</li>
						<li>une présence sur la vidéo d'invitation et sur les emails d'invitation</li>
					</ul>
				</div>
			</div>
		</div>
	</div>
</div>


<!-- Partners -->

<div class="sponsors text-center">
	<div class="container">
		<div class="row">
			<div class="col-xs-12">
				<div class="sponsors-content lined">
					
					<%= render 'partners' %>

				</div>
			</div>
		</div>
	</div>
</div>

<%= render 'contacts' %>
